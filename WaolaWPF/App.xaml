<Application x:Class="WaolaWPF.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:WaolaWPF"
             xmlns:ds="clr-namespace:WaolaWPF.DialogSystem"
             Startup="OnStartup">
    <Application.Resources>
        <ResourceDictionary>
            <ds:MessageBoxPresenter x:Key="{x:Type ds:MessageBoxViewModel}" x:Shared="False"/>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Dictionaries/DictAppStrings.xaml" />
                <ResourceDictionary Source="Dictionaries/DictLastSeenOnlineStrings.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
